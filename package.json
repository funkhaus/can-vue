{
    "name": "can-vue",
    "version": "1.0.2",
    "description": "Canvas + Vue.js, nice and straightforward.",
    "main": "dist/can-vue.js",
    "scripts": {
        "build": "rimraf ./dist && NODE_ENV=production webpack --config ./webpack.config.js",
        "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
        "prepublish": "npm run build",
        "dev": "rimraf ./dist && NODE_ENV=development webpack --watch --config ./webpack.config.js"
    },
    "keywords": [
        "vue",
        "canvas"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/funkhaus/can-vue.git"
    },
    "devDependencies": {
        "autoprefixer": "^9.3.1",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-es2015-spread": "^6.22.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-0": "^6.24.1",
        "css-loader": "^1.0.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "node-sass": "^4.9.4",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.1.0",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "vue": "^2.5.17",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^15.4.2",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.17",
        "webpack": "^4.24.0",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-merge": "^4.1.4"
    },
    "dependencies": {
        "fh-components": "^1.4.5",
        "konva": "^2.5.0"
    }
}
